let sentence = "the dinner was not so bad, I liked it";
let wordNot = sentence.search("not");
console.log(wordNot);
let wordBad = sentence.search("bad");
console.log(wordBad);
if (wordNot < wordBad) {
  let newSen = sentence.replace("not so bad", "good");
  console.log(newSen);
} else {
  console.log(sentence);
}
